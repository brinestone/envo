{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "paths": {
      "@state/*": [
        "./src/app/state/*"
      ],
      "@services/*": [
        "./src/app/services/*"
      ],
      "@components/*": [
        "./src/app/components/*"
      ],
      "@spartan-ng/helm/button": [
        "./libs/ui/ui-button-helm/src/index.ts"
      ],
      "@spartan-ng/helm/sonner": [
        "./libs/ui/ui-sonner-helm/src/index.ts"
      ],
      "@spartan-ng/helm/card": [
        "./libs/ui/ui-card-helm/src/index.ts"
      ],
      "@spartan-ng/helm/typography": [
        "./libs/ui/ui-typography-helm/src/index.ts"
      ],
      "@spartan-ng/helm/form-field": [
        "./libs/ui/ui-form-field-helm/src/index.ts"
      ],
      "@spartan-ng/helm/input": [
        "./libs/ui/ui-input-helm/src/index.ts"
      ],
      "@spartan-ng/helm/separator": [
        "./libs/ui/ui-separator-helm/src/index.ts"
      ],
      "@spartan-ng/helm/select": [
        "./libs/ui/ui-select-helm/src/index.ts"
      ],
      "@spartan-ng/helm/icon": [
        "./libs/ui/ui-icon-helm/src/index.ts"
      ],
      "@spartan-ng/helm/breadcrumb": [
        "./libs/ui/ui-breadcrumb-helm/src/index.ts"
      ],
      "@spartan-ng/helm/menu": [
        "./libs/ui/ui-menu-helm/src/index.ts"
      ],
      "@spartan-ng/helm/skeleton": [
        "./libs/ui/ui-skeleton-helm/src/index.ts"
      ],
      "@spartan-ng/helm/command": [
        "./libs/ui/ui-command-helm/src/index.ts"
      ],
      "@spartan-ng/helm/popover": [
        "./libs/ui/ui-popover-helm/src/index.ts"
      ],
      "@spartan-ng/helm/avatar": [
        "./libs/ui/ui-avatar-helm/src/index.ts"
      ],
      "@spartan-ng/helm/sheet": [
        "./libs/ui/ui-sheet-helm/src/index.ts"
      ],
      "@spartan-ng/helm/badge": [
        "./libs/ui/ui-badge-helm/src/index.ts"
      ],
      "@spartan-ng/helm/accordion": [
        "./libs/ui/ui-accordion-helm/src/index.ts"
      ],
      "@spartan-ng/helm/tabs": [
        "./libs/ui/ui-tabs-helm/src/index.ts"
      ],
      "@spartan-ng/helm/switch": [
        "./libs/ui/ui-switch-helm/src/index.ts"
      ],
      "@spartan-ng/helm/tooltip": [
        "./libs/ui/ui-tooltip-helm/src/index.ts"
      ],
      "@spartan-ng/helm/hover-card": [
        "./libs/ui/ui-hover-card-helm/src/index.ts"
      ],
      "@spartan-ng/helm/alert-dialog": [
        "./libs/ui/ui-alert-dialog-helm/src/index.ts"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
